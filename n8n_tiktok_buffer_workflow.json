{
    "name": "TikTok Buffer Automation",
    "nodes": [
        {
            "parameters": {},
            "id": "manual-trigger",
            "name": "When clicking \"Execute Workflow\"",
            "type": "n8n-nodes-base.manualTrigger",
            "typeVersion": 1,
            "position": [
                250,
                300
            ]
        },
        {
            "parameters": {
                "fileSelector": "/path/to/your/video.mp4"
            },
            "id": "read-binary-file",
            "name": "Read Binary File",
            "type": "n8n-nodes-base.readBinaryFile",
            "typeVersion": 1,
            "position": [
                450,
                300
            ]
        },
        {
            "parameters": {
                "method": "POST",
                "url": "YOUR_ZAPIER_WEBHOOK_URL",
                "sendBinaryData": true,
                "binaryPropertyName": "data",
                "options": {}
            },
            "id": "http-request",
            "name": "Send to Zapier",
            "type": "n8n-nodes-base.httpRequest",
            "typeVersion": 3,
            "position": [
                650,
                300
            ]
        }
    ],
    "connections": {
        "When clicking \"Execute Workflow\"": {
            "main": [
                [
                    {
                        "node": "Read Binary File",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        },
        "Read Binary File": {
            "main": [
                [
                    {
                        "node": "Send to Zapier",
                        "type": "main",
                        "index": 0
                    }
                ]
            ]
        }
    }
}